{
  "keywords": ["rquickjs", "rust", "llrt"],
  "name": "rqjs",
  "version": "0.0.0",
  "main": "index.js",
  "files": ["dist", "readme.md", "package.json"],
  "description": "nodejs modules for rquickjs",
  "scripts": {
    "case-police": "case-police \"**/*.ts\"  fix",
    "dev": "pnpm run --parallel dev",
    "es5": "pnpm run --parallel es5",
    "build": "pnpm run -r build",
    "example": "pnpm run --parallel dev",
    "lint": "biome check --apply-unsafe  ./",
    "format": "biome format --write  ./",
    "lint:fix": "biome lint ./ --write --unsafe",
    "test": "pnpm run --parallel test",
    "clear": "rm -rf node_modules && pnpm -r exec rm -rf node_modules dist es5",
    "clear-dist": "rm -rf dist es5 && pnpm -r exec rm -rf dist es5",
    "release": "pnpm publish -r --access public",
    "release-alpha": "pnpm publish -r --access public --tag alpha"
  },
  "author": "ahaoboy",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@biomejs/biome": "^1.8.3",
    "@chokidar-napi/chokidar": "0.1.3",
    "@deno-ext/console": "^0.1.0",
    "@inquirer/prompts": "^5.1.2",
    "@r-tui/ui": "0.1.5-alpha.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-reconciler": "^0.28.8",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "assert": "^2.1.0",
    "babel-minify": "^0.5.2",
    "case-police": "^0.6.1",
    "chalk": "^5.3.0",
    "core-js": "^3.37.1",
    "deno-console": "^0.1.3",
    "diagnostics_channel": "^1.1.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.0",
